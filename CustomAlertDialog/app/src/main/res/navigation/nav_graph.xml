<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.shakil.customalertdialog.Fragments.Home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/home_fragment" />

    <fragment
        android:id="@+id/navigation_orders"
        android:name="com.example.shakil.customalertdialog.Fragments.Orders.OrdersFragment"
        android:label="Orders"
        tools:layout="@layout/orders_fragment" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.shakil.customalertdialog.Fragments.Notifications.NotificationsFragment"
        android:label="Notifications"
        tools:layout="@layout/notifications_fragment" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.shakil.customalertdialog.Fragments.Profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/profile_fragment" />

</navigation>